<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<ScrollPane fx:id="root" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;"
            stylesheets="@../stylesheets/stylesheet.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientSide.KortosClient">
   <VBox mouseTransparent="true">
      <TextField fx:id="inputText" alignment="TOP_LEFT" maxHeight="1.0" maxWidth="1.0" minHeight="1.0" minWidth="1.0"
                 opacity="0.0" prefHeight="1.0" prefWidth="1.0"/>
      <Text fx:id="consoleText" mouseTransparent="true" strokeType="OUTSIDE" strokeWidth="0.0"
            style="-fx-fill: white;"/>
      <Text fx:id="consoleTextBound" mouseTransparent="true" strokeType="OUTSIDE" strokeWidth="0.0"
            style="-fx-fill: white;"/>
   </VBox>
</ScrollPane>
